{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=ElectricityShopDb;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "RabbitMq": {
    "HostName": "localhost",
    "Port": 5672,
    "UserName": "guest",
    "Password": "guest",
    "VirtualHost": "/",
    "UseSsl": false,
    "ExchangeName": "electricity_shop_exchange",
    "DeadLetterExchange": "electricity_shop_dead_letter_exchange",
    "RetryCount": 3,
    "RetryIntervalSeconds": 30,
    "ConnectionTimeoutSeconds": 15,
    "PrefetchCount": 10
  }
}